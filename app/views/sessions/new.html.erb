Login

<%= form_for @user, url: "/sessions", method: "post" do |f| %>
  <div>
    <span>Email</span> <%= f.text_field :email, placeholder: "example@example.com" %>
  </div>
  <div>
    <span>Password</span> <%= f.password_field :password, placeholder: "notgonnatellyou" %>
  </div>
  <%= f.submit "Log In" %>
<% end %>